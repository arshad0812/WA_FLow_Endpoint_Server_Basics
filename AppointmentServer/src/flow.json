{
    "data_api_version": "3.0",
    "version": "7.2",
    "routing_model": {
        "APPOINTMENT_FORM": [
            "CONFIRM_SCREEN"
        ],
        "CONFIRM_SCREEN": []
    },
    "screens": [
        {
            "id": "APPOINTMENT_FORM",
            "title": "Vleafy Technologies",
            "terminal": false,
            "data": {
                "departments": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "title": {
                                "type": "string"
                            }
                        }
                    },
                    "__example__": [
                        {
                            "id": "cardiology",
                            "title": "Cardiology"
                        },
                        {
                            "id": "dermatology",
                            "title": "Dermatology"
                        },
                        {
                            "id": "neurology",
                            "title": "Neurology"
                        },
                        {
                            "id": "orthopedics",
                            "title": "Orthopedics"
                        }
                    ]
                },
                "doctors": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "title": {
                                "type": "string"
                            }
                        }
                    },
                    "__example__": [
                        {
                            "id": "dr_smith",
                            "title": "Dr. Smith"
                        },
                        {
                            "id": "dr_john",
                            "title": "Dr. John"
                        },
                        {
                            "id": "dr_clara",
                            "title": "Dr. Clara"
                        },
                        {
                            "id": "dr_rohit",
                            "title": "Dr. Rohit"
                        }
                    ]
                },
                "businesstype": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "title": {
                                "type": "string"
                            }
                        }
                    },
                    "__example__": [
                        {
                            "id": "startup",
                            "title": "Startup"
                        },
                        {
                            "id": "small_business",
                            "title": "Small Business"
                        },
                        {
                            "id": "medium_enterprise",
                            "title": "Medium Enterprise"
                        },
                        {
                            "id": "ecommerce_brand",
                            "title": "E-commerce Brand"
                        },
                        {
                            "id": "personal_brand",
                            "title": "Personal Brand / Influencer"
                        },
                        {
                            "id": "agency",
                            "title": "Digital Marketing Agency"
                        },
                        {
                            "id": "local_business",
                            "title": "Local Business / Store"
                        },
                        {
                            "id": "ngo",
                            "title": "Non-Profit / NGO"
                        },
                        {
                            "id": "manufacturer",
                            "title": "Manufacturer / Distributor"
                        },
                        {
                            "id": "freelancer",
                            "title": "Freelancer / Consultant"
                        }
                    ]
                }
            },
            "layout": {
                "type": "SingleColumnLayout",
                "children": [
                    {
                        "type": "Form",
                        "name": "appointment_form",
                        "children": [
                            {
                                "type": "TextHeading",
                                "text": "Appointment Booking"
                            },
                            {
                                "type": "TextBody",
                                "text": "Please fill in your details to schedule your appointment."
                            },
                            {
                                "type": "ImageCarousel",
                                "scale-type": "cover",
                                "images": [
                                    {
                                        "alt-text": "Landscape image",
                                        "src": ""
                                    },
                                    {
                                        "alt-text": "Square image",
                                        "src": ""
                                    },
                                    {
                                        "alt-text": "Portrait image",
                                        "src": ""
                                    }
                                ]
                            },
                            {
                                "type": "TextInput",
                                "label": "Full Name",
                                "name": "name",
                                "input-type": "text",
                                "required": true
                            },
                            {
                                "type": "TextInput",
                                "label": "Phone Number",
                                "name": "number",
                                "input-type": "number",
                                "pattern": "^[0-9]{10}$",
                                "helper-text": "Enter a 10-digit valid phone number",
                                "required": true
                            },
                            {
                                "type": "TextInput",
                                "label": "Enter Your Age",
                                "name": "age",
                                "input-type": "number",
                                "pattern": "^(?:[1-9]|[1-9][0-9])$",
                                "helper-text": "Enter your age (1–99 only)",
                                "required": true
                            },
                            {
                                "type": "RadioButtonsGroup",
                                "name": "gender",
                                "label": "Select Your Gender",
                                "required": true,
                                "data-source": [
                                    {
                                        "id": "Men",
                                        "title": "Men"
                                    },
                                    {
                                        "id": "Women",
                                        "title": "Women"
                                    },
                                    {
                                        "id": "Other",
                                        "title": "Other"
                                    },
                                    {
                                        "id": "Prefer Not to say",
                                        "title": "Prefer Not to say"
                                    }
                                ]
                            },
                            {
                                "type": "Dropdown",
                                "label": "Department",
                                "name": "department",
                                "data-source": "${data.departments}",
                                "required": true,
                                "on-select-action": {
                                    "name": "data_exchange",
                                    "payload": {
                                        "department": "${form.department}"
                                    }
                                }
                            },
                            {
                                "type": "Dropdown",
                                "label": "Doctor",
                                "name": "doctor",
                                "data-source": "${data.doctors}",
                                "required": true,
                                "on-select-action": {
                                    "name": "data_exchange",
                                    "payload": {
                                        "doctor": "${form.doctor}"
                                    }
                                }
                            },
                            {
                                "type": "DatePicker",
                                "label": "Select Appointment Date",
                                "name": "appointmentdate",
                                "required": true
                            },
                            {
                                "type": "TextInput",
                                "label": "Preferred Time",
                                "name": "time",
                                "input-type": "text",
                                "required": true
                            },
                            {
                                "type": "TextInput",
                                "label": "Describe ur symptoms",
                                "name": "symptoms",
                                "input-type": "text"
                            },
                            {
                                "type": "ChipsSelector",
                                "name": "chips",
                                "label": "Consultation Mode",
                                "description": "Select how you’d like to attend your consultation",
                                "max-selected-items": 1,
                                "data-source": [
                                    {
                                        "id": "in_person",
                                        "title": "In-Person"
                                    },
                                    {
                                        "id": "video_call",
                                        "title": "Video Call"
                                    },
                                    {
                                        "id": "phone_call",
                                        "title": "Phone Call"
                                    }
                                ]
                            },
                            {
                                "type": "Footer",
                                "label": "Continue",
                                "on-click-action": {
                                    "name": "data_exchange",
                                    "payload": {
                                        "next": "CONFIRM_SCREEN",
                                        "form": "${form}"
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        },
        {
            "id": "CONFIRM_SCREEN",
            "title": "Thank You!",
            "terminal": true,
            "layout": {
                "type": "SingleColumnLayout",
                "children": [
                    {
                        "type": "Form",
                        "name": "confirmation_form",
                        "children": [
                            {
                                "type": "TextBody",
                                "text": "Thanks for booking your appointment! Our team will contact you soon with confirmation details."
                            },
                            {
                                "type": "Footer",
                                "label": "Finish",
                                "on-click-action": {
                                    "name": "data_exchange",
                                    "payload": {
                                        "complete": true
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        }
    ]
}